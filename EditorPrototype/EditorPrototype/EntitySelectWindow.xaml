<Window x:Class="EditorPrototype.EntitySelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EditorPrototype"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="Entity selection" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button x:Name="vertexAddBut" Content="Vertex" HorizontalAlignment="Left" Margin="0,0,-0.4,0" VerticalAlignment="Top" Width="294" Height="135" Click="vertexAddBut_Click"/>
        <Button x:Name="edgeAddBut" Content="Edge" HorizontalAlignment="Left" Margin="0,135,-0.4,0" VerticalAlignment="Top" Width="294" Height="135" Click="edgeAddBut_Click"/>

    </Grid>
</Window>
